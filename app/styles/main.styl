html, body
  height: 100%
.container
  width: 100%
  height: 100%
  svg
    width: 100%
    height: 100%

  .bar
    display: inline-block;
    width: 20px;
    height: 75px;
    margin-right: 3px;
    background-color: RoyalBlue;
.stock-graph
  width: 1024px
  height: 500px 
  .stock-info
    text
      font-size: 20px
    .text-info
      fill: black
  .graph
    height: 100%
    width: 960px
    .axis
      path
        fill: none
        stroke: #000
    .rect-green
      fill: green
    .rect-red
      fill: red
  rect.pane
    fill: #fff
    opacity: 0



